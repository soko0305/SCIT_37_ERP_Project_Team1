<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.erp.dao.ProductionMapper">
	<select id="productMaterialSelect" parameterType="com.project.erp.vo.ProductMaterialVO" resultType="com.project.erp.vo.ProductMaterialVO">
	select
		product_material_code,
		pd_code,
		rawm_code,
		product_material_amount
	from
		PRODUCT_MATERIAL
	where
		pd_code=#{pd_code}
	</select>
</mapper>
